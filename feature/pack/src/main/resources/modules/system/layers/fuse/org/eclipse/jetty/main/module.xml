<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.1" name="org.eclipse.jetty">
  <properties>
    <property name="jboss.api" value="private" />
  </properties>
  <resources>
    <artifact name="${org.eclipse.jetty:jetty-client}" />
    <artifact name="${org.eclipse.jetty:jetty-http}" />
    <artifact name="${org.eclipse.jetty:jetty-io}" />
    <artifact name="${org.eclipse.jetty:jetty-security}" />
    <artifact name="${org.eclipse.jetty:jetty-servlet}" />
    <artifact name="${org.eclipse.jetty:jetty-util}" />
    <artifact name="${org.eclipse.jetty:jetty-util-ajax}" />
  </resources>
  <dependencies>
    <module name="javax.api" />
    <module name="org.eclipse.jetty.server" />
    <module name="javax.servlet.api" />
  </dependencies>
  <exports>
    <exclude path="org/eclipse" />
  </exports>
</module>
